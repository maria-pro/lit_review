@article{ WOS:000501739000017,
Author = {O'Reilly, Michelle and Dogra, Nisha and Hughes, Jason and Reilly, Paul
   and George, Riya and Whiteman, Natasha},
Title = {Potential of social media in promoting mental health in adolescents},
