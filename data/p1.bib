@article{ WOS:000419836100001,
Author = {Marrapodi, Elisabeth Jacobsen},
Title = {Use of Social Media for Consumer Health Outreach in a Solo Hospital
   Library},
Journal = {JOURNAL OF CONSUMER HEALTH ON THE INTERNET},
Year = {2017},
Volume = {21},
Number = {4},
Pages = {325-337},
Abstract = {Using social media in hospital libraries is an excellent tool for
   reaching and engaging existing or potential users. However, small or
   solo hospital librarians do not always have enough staff or time to
   incorporate these resources into their menu of services. Hospital
   librarians should consider the potential benefits of using the library
   as a trusted social media resource for the community and can customize
   outreach without sacrificing staff time, straining the budget, or
   battling the firewall. Having the hospital library offer health
   information to the community through social media has many potential
   benefits for improving health knowledge and health literacy.},
