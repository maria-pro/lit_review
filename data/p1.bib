@article{ WOS:000431148100001,
Author = {Young, Sean D.},
Title = {Social Media as a New Vital Sign: Commentary},
Journal = {JOURNAL OF MEDICAL INTERNET RESEARCH},
Year = {2018},
Volume = {20},
Number = {4},
Month = {APR},
Abstract = {Mobile technologies, such as wireless glucometers and mobile health
   apps, are increasingly being integrated into health and medical care.
   Because patients openly share real-time information about their health
   behaviors and outcomes on social media, social media data may also be
   used as a tool for monitoring patient care. This commentary describes
   how recent advances in computer science, psychology, and medicine enable
   social media data to become a new health ``vital sign,{''} as well as
   actionable steps that public health officials, health systems, and
   clinics can take to integrate social data into both public and
   population health as well as into individual patient care. Barriers that
   first need to be addressed, including privacy concerns, legal and
   ethical responsibilities, and infrastructure support, are discussed.},
