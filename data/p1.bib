@article{ WOS:000517806600011,
Author = {Griauzde, Dina H. and Kieffer, Edith C. and Domoff, Sarah E. and Hess,
   Kristen and Feinstein, Susannah and Frank, Amy and Pike, Denise and
   Pesch, Megan H.},
Title = {The influence of social media on child feeding practices and beliefs
   among Hispanic mothers: A mixed methods study},
Journal = {EATING BEHAVIORS},
Year = {2020},
Volume = {36},
Month = {JAN},
Abstract = {Objectives: (1) To identify factors that influence child feeding
   practices and beliefs among Hispanic mothers in a low-income community;
   (2) to describe the use of social media, other internet websites, and
   text messaging among Hispanic mothers; and (3) to explore mothers'
   perceptions of social media and/or text messaging interventions to
   prevent childhood obesity.
   Methods: Mixed methods descriptive study with a sequential explanatory
   design. Hispanic mothers (N = 66) from Detroit, Michigan with children
   between 6 and 36 months of age completed surveys regarding their child
   feeding practices, the source (s) influencing these practices, and their
   use of social media, internet, and text messaging. During qualitative
   interviews (N = 19), we explored mothers' use of social media and
   internet websites to find child health information as well as mothers'
   perspectives on social media/internet/text messaging interventions to
   promote child health.
   Results: Most survey respondents were between the ages of 20 and 39
   years. One-third of mothers breastfed their child for > 6 months; 68\%
   did not introduce solids until their child was older than 6 months. The
   majority (96\%) owned a cellphone; 75\% used social media at least once
