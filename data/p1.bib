@article{ WOS:000211179700001,
Author = {Spence, Patric and Lachlan, Kenneth and Westerman, David and Spates,
   Stephen},
Title = {Where the Gates Matter Less: Ethnicity and Perceived Source Credibility
   in Social Media Health Messages},
Journal = {HOWARD JOURNAL OF COMMUNICATIONS},
Year = {2013},
Volume = {24},
Number = {1},
Pages = {1-16},
Abstract = {Research indicates that minority populations-who have a greater tendency
   to mistrust health information unless it's delivered through
   interpersonal contacts-are using social media as a means of
   health-related information seeking. The current study examines how
   individuals make credibility judgments about health-related information
   from social media sites. Specifically, reposts of health information on
   social media were examined. A 2 x 2 experimental design tested the
   relationship between the race of the person reposting the health
   information and respondents' race on perceptions of credibility. Results
   found differences in perceived credibility based on both the race of the
   profile owner and race of judge. Moreover, these results suggest that
   the source of the social media user-generated cue has an effect in the
   credibility of reposting of health-related information. Implications of
   these findings are discussed.},
