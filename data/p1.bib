@article{ WOS:000677671400001,
Author = {Marino, Maria Ines and Adogba, Oluwabusola and Tilli, Nicolas},
Title = {Mental Health Interventions: Reflections on a Radio Program as
   Alternative for Stigma Reduction and Social Integration},
Journal = {FRONTIERS IN COMMUNICATION},
Year = {2020},
Volume = {5},
Month = {OCT 21},
Abstract = {Under the umbrella of Social Identity Theory and Goffman's notion of
   stigma, this paper reviews research on stigma in connection with social
   perceptions of individuals with mental health problems and the role of
   the media in building those perceptions. The study specifically examined
   three aspects: (1) social stigma of individuals with mental health
   problems; (2) media as an alternative intervention for individuals with
   mental health problems, and (3) alternative interventions for stigma
