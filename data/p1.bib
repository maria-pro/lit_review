@article{ WOS:000447074900010,
Author = {Gupta, Anita and Tyagi, Mukta and Sharma, Deepshikha},
Title = {Use of Social Media Marketing in Healthcare},
Journal = {JOURNAL OF HEALTH MANAGEMENT},
