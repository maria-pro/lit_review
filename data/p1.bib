@article{ WOS:000314364400014,
Author = {Evers, Clifton Westly and Albury, Kath and Byron, Paul and Crawford,
   Kate},
Title = {Young People, Social Media, Social Network Sites and Sexual Health
   Communication in Australia: ``This is Funny, You Should Watch It{''}},
Journal = {INTERNATIONAL JOURNAL OF COMMUNICATION},
Year = {2013},
Volume = {7},
Pages = {263-280},
Abstract = {Social media and social network sites (SNS) are an evolving area for
   sexual health communication with young people. They present
   opportunities and challenges for sexual health professionals and young
   people alike, such as learning through interactivity and addressing
   concerns about privacy. In this article, we present and discuss the
   findings from six rural and urban focus groups with young people in
   Australia about the use of social media and SNS for sexual health
   communication. We discuss a number of issues related to the use of
   social media and SNS for sexual health communication, such as concerns
   about bullying, privacy, and the stigma attached to sexual health.},
