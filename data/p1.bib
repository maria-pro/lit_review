@article{ WOS:000350266800012,
Author = {Gagnon, Kendra and Sabus, Carla},
Title = {Professionalism in a Digital Age: Opportunities and Considerations for
   Using Social Media in Health Care},
Journal = {PHYSICAL THERAPY},
