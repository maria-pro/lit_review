@article{ WOS:000477732300005,
Author = {Walter, Nathan and Robbins, Chris and Murphy, Sheila T. and
   Ball-Rokeach, Sandra J.},
Title = {The weight of networks: the role of social ties and ethnic media in
   mitigating obesity and hypertension among Latinas},
Journal = {ETHNICITY \& HEALTH},
