@article{ WOS:000558289600001,
Author = {Shang, Lili and Zhou, Junjie and Zuo, Meiyun},
Title = {Understanding older adults' intention to share health information on
   social media: the role of health belief and information processing},
Journal = {INTERNET RESEARCH},
Year = {2021},
Volume = {31},
