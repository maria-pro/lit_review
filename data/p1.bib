@article{ WOS:000657097900009,
Author = {Alhusseini, Noara and Banta, Jim E. and Oh, Jisoo and Montgomery,
   Susanne B.},
Title = {Social Media Use for Health Purposes by Chronic Disease Patients in the
   United States},
Journal = {SAUDI JOURNAL OF MEDICINE \& MEDICAL SCIENCES},
Year = {2021},
