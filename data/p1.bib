@article{ WOS:000393296000008,
Author = {Jackson, Devlon N. and Chou, Wen-Ying Sylvia and Coa, Kisha I. and Oh,
   April and Hesse, Bradford},
Title = {Implications of social media use on health information technology
   engagement: Data from HINTS 4 cycle 3},
Journal = {TRANSLATIONAL BEHAVIORAL MEDICINE},
Year = {2016},
Volume = {6},
Number = {4},
Pages = {566-576},
Month = {DEC},
Abstract = {Little is known about the association between Internet/social media use
   and health information technology (HIT) engagement. This study examines
   patterns of social media use and HIT engagement in the U.S.A. using data
   from the 2013 Health Information National Trends Survey (N = 3,164).
   Specifically, predictors of two HIT activities (i.e., communicating with
   a healthcare provider using the Internet or email and tracking personal
   health information electronically) are examined. Persons who were
   females, higher education, non-Hispanic others, having a regular
   healthcare provider, and ages 35-44 were more likely to participate in
   HIT activities. After controlling for sociode-mographics and health
   correlates, social media use was significantly associated with HIT
   engagement. To our knowledge, this is one of the first studies to
   systematically examine the use and relationships across multiple types
   of health-related online media.},
