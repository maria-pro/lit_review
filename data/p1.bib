@article{ WOS:000466739800003,
Author = {Ormsby, Hollie and Owen, Alison Leah and Bhogal, Manpal Singh},
Title = {A brief report on the associations amongst social media use, gender, and
   body esteem in a UK student sample},
Journal = {CURRENT PSYCHOLOGY},
Year = {2019},
Volume = {38},
Number = {2},
Pages = {303-307},
Month = {APR},
Abstract = {Research into the effects of social media on personal wellbeing have
   been controversial in recent years, with research highlighting links
   between social media use and body esteem. This conceptual replication
   study aimed to explore relationships amongst social media use, body
   esteem, and gender amongst UK university students (n=100). Participants
   completed measures of social media use and body esteem. It was
   hypothesized that social media intensity and usage would negatively
   predict body esteem, with high social media intensity relating to lower
   body esteem. We find that gender was the only significant predictor of
   body esteem, with women having lower body esteem compared to men. We
   were unable to replicate previous findings, as our findings show no
   relationships amongst social media intensity, use, and body esteem.},
