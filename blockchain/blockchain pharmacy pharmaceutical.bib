@inproceedings{ WOS:000470211600103,
Author = {Thatcher, Camden and Acharya, Subrata},
Book-Group-Author = {IEEE},
Title = {Pharmaceutical uses of Blockchain Technology},
Booktitle = {2018 IEEE INTERNATIONAL CONFERENCE ON ADVANCED NETWORKS AND
   TELECOMMUNICATIONS SYSTEMS (ANTS)},
Year = {2018},
Note = {12th IEEE International Conference on Advanced Networks and
   Telecommunications Systems (ANTS), Indore, INDIA, DEC 16-19, 2018},
Organization = {IEEE; IEEE Commun Soc; CISCO; WIPRO; Aricent; IEEE Standards Assoc; IEEE
   Future Networks; Sci \& Engn Res Board; Govt India, Dept Sci \& Technol;
   Govt India, Def Res Dev Org; UK India Educ \& Res Initiat; C DoT; CSIR;
   MathWorks},
Abstract = {This research identifies a conceptual e-Prescription system based on
   blockchain technology. The system utilizes cryptocurrency principles
   applied to the electronic prescription processes to achieve its goals.
   This e-Prescription system can be utilized as a combatant to the
   nationwide opioid crisis as a potential Prescription Drug Monitoring
   System (PDMP). While not a fully evaluated system, the proof of concept
   results presented have significant demonstrated potential. Moreover, the
