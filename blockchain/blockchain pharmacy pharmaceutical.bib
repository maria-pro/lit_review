@article{ WOS:000504282400461,
Author = {Casino, Fran and Kanakaris, Venetis and Dasaklis, Thomas K. and
   Moschuris, Socrates and Rachaniotis, Nikolaos P.},
Title = {Modeling food supply chain traceability based on blockchain technology},
