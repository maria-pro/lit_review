@article{ WOS:000556946200001,
Author = {Singh, Rajani and Dwivedi, Ashutosh Dhar and Srivastava, Gautam},
Title = {Internet of Things Based Blockchain for Temperature Monitoring and
   Counterfeit Pharmaceutical Prevention},
