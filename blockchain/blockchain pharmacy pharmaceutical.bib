@inproceedings{ WOS:000465515000004,
Author = {Di Ciccio, Claudio and Cecconi, Alessio and Mendling, Jan and Felix,
   Dominik and Haas, Dominik and Lilek, Daniel and Riel, Florian and Rumpl,
   Andreas and Uhlig, Philipp},
