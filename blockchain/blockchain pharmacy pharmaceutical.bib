Booktitle = {PROCEEDINGS OF THE 2019 INTERNATIONAL CONFERENCE ON INDUSTRIAL
   ENGINEERING AND SYSTEMS MANAGEMENT (IESM 2019)},
Year = {2019},
Pages = {458-463},
Note = {International Conference on Industrial Engineering and Systems
