@article{ WOS:000591839100001,
Author = {Tan, Lorwai and Tivey, David and Kopunic, Helena and Babidge, Wendy and
   Langley, Sally and Maddern, Guy},
Title = {Part 2: blockchain technology in health care},
Journal = {ANZ JOURNAL OF SURGERY},
Year = {2020},
