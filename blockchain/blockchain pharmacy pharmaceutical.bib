@article{ WOS:000506382200001,
Author = {Hastig, Gabriella M. and Sodhi, ManMohan S.},
Title = {Blockchain for Supply Chain Traceability: Business Requirements and
   Critical Success Factors},
Journal = {PRODUCTION AND OPERATIONS MANAGEMENT},
