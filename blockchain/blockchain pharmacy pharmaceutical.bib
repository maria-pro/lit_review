@inproceedings{ WOS:000607176800029,
Author = {Badr, Nabil Georges},
Editor = {Bernardino, J and Salgado, A and Filipe, J},
Title = {Blockchain or Distributed Ledger Technology What Is in It for the
   Healthcare Industry?},
Booktitle = {KMIS: PROCEEDINGS OF THE 11TH INTERNATIONAL JOINT CONFERENCE ON
   KNOWLEDGE DISCOVERY, KNOWLEDGE ENGINEERING AND KNOWLEDGE MANAGEMENT, VOL
   3: KMIS},
