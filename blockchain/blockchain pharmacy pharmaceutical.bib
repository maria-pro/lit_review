@article{ WOS:000886165400001,
Author = {Alnuaimi, Aysha and Alshehhi, Amna and Salah, Khaled and Jayaraman, Raja
   and Omar, Ilhaam A. and Battah, Ammar},
Title = {Blockchain-Based Processing of Health Insurance Claims for Prescription
   Drugs},
Journal = {IEEE ACCESS},
