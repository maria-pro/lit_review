@article{ WOS:000696153600004,
Author = {Sadeghi, Mahsa and Mahmoudi, Amin and Deng, Xiaopeng},
Title = {Adopting distributed ledger technology for the sustainable construction
   industry: evaluating the barriers using Ordinal Priority Approach},
